<?xml version="1.0" encoding="UTF-8"?>
<arch>
    <archname>cortex-m4</archname>
    <compile-option>
        <mcpu>cortex-m4</mcpu>
    </compile-option>
    <!--下面是架构中规定由厂家可选实现的部分，创建CPU时，提示用户配置-->
